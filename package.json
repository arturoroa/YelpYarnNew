{
  "name": "yelp-click-testing-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently  \"npm run dev:minimal-backend\"    \"npm run dev:frontend\"  ",
    "dev:frontend": "vite",
    "dev:minimal-backend": "node backend/minimal-server.js",
    "dev:backend": "nodemon --watch backend --ext ts --exec \"npm run build:backend && node backend/dist/server.js\"",
    "build:backend": "tsc --project backend/tsconfig.json",
    "build": "vite build",
    "start": "node backend/dist/server.js",
    "preview": "vite preview",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "lucide": "^0.544.0",
    "lucide-react": "^0.544.0",
    "puppeteer": "^22.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/js": "^9.9.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.6.2",
    "@types/puppeteer": "^5.4.7",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^9.2.1",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "jest": "^30.2.0",
    "jest-environment-node": "^30.2.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.4.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^7.1.7"
  }
}
